# BSD-ROMFS directory layout
helpstring="for BSD embedded devices with ROM filesystems"
opbxprefix=
opbxexecdir=/usr/libexec
opbxutilsdir=/usr/sbin
opbxconfdir=/etc/callweaver
opbxlibdir=/usr/lib/callweaver
opbxvardir=/var/lib/callweaver
opbxdatadir=/usr/share/callweaver
opbxlogdir=/var/log/callweaver
opbxrundir=/var/run
opbxspooldir=/var/spool/callweaver
opbxmandir=/usr/share/man
opbxdocdir=/usr/share/doc
