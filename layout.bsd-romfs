# BSD-ROMFS directory layout
helpstring="for BSD embedded devices with ROM filesystems"
opbxprefix=
opbxexecdir=/usr/libexec
opbxutilsdir=/usr/sbin
opbxconfdir=/etc/callweaver.org
opbxlibdir=/usr/lib/callweaver.org
opbxvardir=/var/lib/callweaver.org
opbxdatadir=/usr/share/callweaver.org
opbxlogdir=/var/log/callweaver.org
opbxrundir=/var/run
opbxspooldir=/var/spool/callweaver.org
opbxmandir=/usr/share/man
opbxdocdir=/usr/share/doc
