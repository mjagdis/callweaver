[general]
;user to run callweaver (this can not be root/UID 0)
opbxrunuser => @opbxrunuser@
;group to run callweaver (this can not be root wheel or GID 0)
opbxrungroup => @opbxrunuser@

[directories]
opbxetcdir => @opbxconfdir@
opbxmoddir => @opbxmoddir@
opbxvarlibdir => @opbxvardir@
opbxdbdir => @opbxdbdir@
opbxogidir => @opbxogidir@
opbxspooldir => @opbxspooldir@
opbxrundir => @opbxrundir@
opbxlogdir => @opbxlogdir@
opbxsoundsdir => @opbxsoundsdir@

; Changing the following lines may compromise your security.
;[files]
;opbxctlpermissions = 0660
;opbxctlowner = root
;opbxctlgroup = apache
;opbxctl = callweaver.ctl
;opbxdb = @opbxdbfile@
