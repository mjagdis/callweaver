[general]
;user to run callweaver (this can not be root/UID 0)
cwrunuser => @opbxrunuser@
;group to run callweaver (this can not be root wheel or GID 0)
cwrungroup => @opbxrunuser@

[directories]
cwetcdir => @opbxconfdir@
cwmoddir => @opbxmoddir@
cwvarlibdir => @opbxvardir@
cwdbdir => @opbxdbdir@
cwogidir => @opbxogidir@
cwspooldir => @opbxspooldir@
cwrundir => @opbxrundir@
cwlogdir => @opbxlogdir@
cwsoundsdir => @opbxsoundsdir@
cwkeydir => @opbxkeydir@

; Changing the following lines may compromise your security.
;[files]
;cwctlpermissions = 0660
;cwctlowner = root
;cwctlgroup = apache
;cwctl = callweaver.ctl
;cwdb = @opbxdbfile@
